>\n                    <p class=\"title\">Update Equipment #{{$route.params.tg}}</p>\n                    <section>\n                        <div class=\"columns\">\n                            <div class=\"column\">\n                                <b-field label=\"Title\">\n                                    <b-input :value=\"equip[0].Title\"\n                                             @input=\"updateTitle\"\n                                             placeholder=\"Enter title\"\n                                             rounded\n                                             size=\"is-small\"\n                                    ></b-input>\n                                </b-field>\n                            </div>\n                            <div class=\"column\">\n                                <b-field label=\"Type\">\n                                    <b-select placeholder=\"Select a type\"\n                                              :value=\"equip[0].Type\"\n                                              @input=\"updateType\"\n                                              rounded\n                                              size=\"is-small\"\n                                    >\n                                        <option v-for=\"(option, key) in options.type\"\n                                                :key=\"key\"\n                                        >\n                                            {{option}}\n                                        </option>\n                                    </b-select>\n                                </b-field>\n                            </div>\n                            <div class=\"column\">\n                                <b-field label=\"Sub Type\">\n                                    <b-select placeholder=\"Select a socket type\"\n                                              :value=\"equip[0].SubType\"\n                                              @input=\"updateSubType\"\