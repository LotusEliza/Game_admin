                      @input=\"updateSockets\"\n                                                   placeholder=\"Enter sockets\"\n                                                   rounded\n                                                   size=\"is-small\"\n                                    ></b-numberinput>\n                                </b-field>\n                            </div>\n                            <div class=\"column\">\n                                <b-field label=\"Buy price\">\n                                    <b-numberinput :value=\"equip[0].BuyPrice\"\n                                                   @input=\"updateBuyPrice\"\n                                                   placeholder=\"Enter buy price\"\n                                                   rounded\n                                                   size=\"is-small\"\n                                    ></b-numberinput>\n                                </b-field>\n                            </div>\n                            <div class=\"column\">\n                                <b-field label=\"Sell price\">\n                                    <b-numberinput\n                                            :value=\"equip[0].SellPrice\"\n                                            @input=\"updateSellPrice\"\n                                            placeholder=\"Enter sell price\"\n                                            rounded\n                                            size=\"is-small\"\n                                    ></b-numberinput>\n                                </b-field>\n                            </div>\n                        </div>\n                        <div class=\"columns\">\n                            <div class=\"column\">\n                                <b-field label=\"Reputation\">\n                                    <b-numberinput :value=\"equip[0].Reputation\"\n                                             @input=\"updateReputation\"\n                                             placeholder=\"Enter reputation\"\n                                             rounded\n                                             size=\"is-small\"\n                                    ></b-numberinput>\n                                </b-field>\n                            </div>\n                            <div class=\"column\">\n                                <b-field label=\"Damage\">\n                                    <b-numberinput :value=\"equip[0].Damage\"\n                                                   @input=\"updateDamage\"\n                                                   placeholder=\"Enter damage\"\n                                                   rounded\n                                                   size=\"is-small\"\n                                    ></b-numberinput>\n                                </b-field>\n                            </div>\n                            <div class=\"column\">\n                                <b-field label=\"Armor\">\n                                    <b-numberinput :value=\"equip[0].Armor\"\n                                                   @input=\"updateArmor\"\n                                                   placeholder=\"Enter armor\"\n                                                   rounded\n                                                   size=\"is-small\"\n                                    ></b-numberinput>\n                                </b-field>\n                            </div>\n                        </div>\n                        <div class=\"columns\">\n                            <div class=\"column\">\n                                <b-field label=\"Air\">\n                                    <b-numberinput :value=\"equip[0].Air\"\n                                                   @input=\"updateAir\"\n                                                   placeholder=\"Enter air\"\n                                                   rounded\n                                                   size=\"is-small\"\n                                    ></b-numberinput>\n                                </b-field>\n                            </div>\n                            <div class=\"column\">\n                                <b-field label=\"Mine\">\n                                    <b-numberinput :value=\"equip[0].Mine\"\n                                                   @input=\"updateMine\"\n                                                   placeholder=\"Enter mine\"\n                                                   rounded\n                                                   size=\"is-small\"\n                                    ></b-numberinput>\n                                </b-field>\n                            </div>\n                            <div class=\"column\">\n                                <b-field label=\"Time\">\n                                    <b-numberinput :value=\"equip[0].Time\"\n                                                   @input=\"updateTime\"\n                                                   placeholder=\"Enter time\"\n                                                   rounded\n                                                   size=\"is-small\"\n                                    ></b-numberinput>\n                                </b-field>\n                            </div>\n                        </div>\n                    </section>\n                    <section>\n                        <div class=\"buttons pt-2\" >\n                            <b-button  @click=\"updateEquip()\" type=\"is-success\">Submit</b-button>\n                            <b-button tag=\"router-link\"\n                                      to=\"/equipments\"\n                                      type=\"is-link is-light\"\n                            >\n                                Back\n                            </b-button>\n                        </div>\n                    </section>\n\n                </div>\n                <div class=\"tile is-child\">\n                    <notification :message=\"message\"></notification>\n                </div>\n\n            </div>\n            <div class=\"tile is-2 is-vertical is-parent\">\n            </div>\n        </div>\n<!--            <notification :message=\"message\"></notification>-->\n        </div>\n\n        <div v-else>{{redPage()}}</div>\n    </div>\n</template>\n\n<script>\n    import Notification from \"../components/Notification\";\n\n    export default {\n        name: 'updateEquip',\n        components: {\n            'notification': Notification,\n        },\n        data() {\n            return {\n                options: {\n                    \"type\": ['weapon', 'armor', 'balloon', 'console'],\n                    \"subType\": [\n                        {text: 'boer', value: 0},\n                        {text: 'nullt', value: 1},\n                    ],\n                    \"socketType\": [\n                        {text: 'damage', value: 1},\n                        {text: 'armor', value: 1},\n                        {text: 'hp', value: 2},\n                        {text: 'mine', value: 3}],\n                },\n                message: '',\n            }\n        },\n        mounted(){\n            this.$store.commit('SET_EQUIP_ID', this.$route.params.tg)\n        },\n        methods: {\n            updateEquip(){\n                this.message = 'The equip is updated!';\n                this.$store.dispatch('updateEquip');\n            },\n            updateTitle (e) {\n                this.$store.commit('UPDATE_TITLE', e)\n            },\n            updateType(e){\n                this.$store.commit('UPDATE_TYPE', e);\n            },\n            updateSubType(e){\n                this.$store.commit('UPDATE_SUBTYPE', e)\n            },\n            updateBuyPrice(e){\n                this.$store.commit('UPDATE_BUY_PRICE', e)\n            },\n            updateSellPrice(e){\n                this.$store.commit('UPDATE_SELL_PRICE', e)\n            },\n            updateReputation(e){\n                this.$store.commit('UPDATE_REPUTATION', e)\n            },\n            updateDamage(e){\n                this.$store.commit('UPDATE_DAMAGE', e)\n            },\n            updateArmor(e){\n                this.$store.commit('UPDATE_ARMOR', e)\n            },\n            updateAir(e){\n                this.$store.commit('UPDATE_AIR', e)\n            },\n            updateMine(e){\n                this.$store.commit('UPDATE_MINE', e)\n            },\n            updateTime(e){\n                this.$store.commit('UPDATE_TIME', e)\n            },\n            updateSocketType(e){\n                this.$store.commit('UPDATE_SOCKET_TYPE', e)\n            },\n            updateSockets(e){\n                this.$store.commit('UPDATE_SOCKETS', e)\n            },\n            redPage(){\n                console.log('No data!')\n                this.$router.push({path: '/equipments'});\n            },\n        },\n        computed: {\n            equip() {\n                    return this.$store.getters.equip(this.$route.params.tg)\n                }\n        },\n    }\n</script>\n\n<style>\n    /*.p-3 {*/\n    /*    padding: 20px !important;*/\n    /*}*/\n</style>\n"]}]}